keybind=cmd+s>n=new_tab
keybind=ctrl+a>o=toggle_tab_overview
keybind=ctrl+a>n=next_tab
keybind=ctrl+a>p=previous_tab
keybind=ctrl+a>right=new_split:right
keybind=ctrl+a>down=new_split:down
keybind=shift+ctrl+right=goto_split:right
keybind=shift+ctrl+left=goto_split:left
keybind=shift+ctrl+up=goto_split:up
keybind=shift+ctrl+down=goto_split:down

keybind=cmd+s>r=reload_config
keybind=ctrl+a>e=equalize_splits